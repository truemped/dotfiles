[alias]
    ap = add --patch
    b = branch -vv
    ci = commit
    co = checkout
    dc = diff --cached
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%an, %cr)%Creset' --abbrev-commit --date=relative
    st = status
[core]
    editor = /usr/local/bin/vim
    excludesfile = ~/.gitignore
[color]
    status = auto
    branch = auto
    diff = auto
[push]
	default = tracking
